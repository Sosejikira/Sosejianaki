		ContainerLayout.Name = 'Layout'
		ContainerLayout.SortOrder = Enum.SortOrder.LayoutOrder
		ContainerLayout.Padding = UDim.new(0, -5)

		-- Connections
		NewTabButton.MouseButton1Click:Connect(function()
			for Key, Children in pairs(ContainerFolder:GetChildren()) do
				if Children:IsA('ScrollingFrame') then
					Children.Visible = false
				end
				NewContainer.Visible = true
			end
			--
			for Key, Children in pairs(Tabs:GetChildren()) do
				if Children:IsA('TextButton') then
					for Key, Children in pairs(Children:GetChildren()) do
						if Children.Name == 'Image' then
							game:GetService('TweenService'):Create(Children, TweenInfo.new(.25), {ImageColor3 = Color3.fromRGB(150, 150, 150)}):Play()
						elseif Children.Name == 'Frame' then
							game:GetService('TweenService'):Create(Children, TweenInfo.new(.25), {TextColor3 = Color3.fromRGB(150, 150, 150)}):Play()
						end
					end
				end
			end

			-- Tweens
			game:GetService('TweenService'):Create(NewTabImage, TweenInfo.new(.25), {ImageColor3 = Color3.fromRGB(250, 250, 250)}):Play()
			game:GetService('TweenService'):Create(NewTabLabel, TweenInfo.new(.25), {TextColor3 = Color3.fromRGB(250, 250, 250)}):Play()
		end)

