			-- Canvas
			local NewCanvasSize = UDim2.new(0, 0, 0, ContainerLayout.AbsoluteContentSize.Y)
			game:GetService('TweenService'):Create(NewContainer, TweenInfo.new(.25), {CanvasSize = NewCanvasSize}):Play()
		end
		--
		return Contents
	end
	return Categories
end
function Sosejianaki:SetBind(Keybind)
	if Keybind.Name ~= "Unknown" then
		Minimize = Keybind
	end
end

return Sosejianaki
