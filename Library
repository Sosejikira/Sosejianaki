			local ContentLabel = Instance.new('TextLabel', ContentFrame)
			ContentLabel.Name = 'Frame'
			ContentLabel.AnchorPoint = Vector2.new(.5, .5)
			ContentLabel.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
			ContentLabel.BackgroundTransparency = 0
