local Sosejianaki = {}

-- Functions
local Mouse = game:GetService('Players').LocalPlayer:GetMouse()
local Minimize = Enum.KeyCode.RightControl
local Library = Instance.new('ScreenGui')
Library.Name = 'Library'
Library.Parent = game:GetService('GoreGui')
Library.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
function MakeDraggable(Topbar, Object)
	-- Functions
	local Dragging = nil
	local DragInput = nil
	local DragStart = nil
	local StartPosition = nil
	--
	local function Update(Input)
		local Delta = Input.Position - DragStart
		local Position = UDim2.new(StartPosition.X.Scale, StartPosition.X.Offset + Delta.X, StartPosition.Y.Scale, StartPosition.Y.Offset + Delta.Y)
		game:GetService('TweenService'):Create(Object, TweenInfo.new(.25), {Position = Position}):Play()
	end
	
	-- Connections
	Topbar.InputBegan:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
			Dragging = true
			DragStart = Input.Position
			StartPosition = Object.Position
			Input.Changed:Connect(function()
				if Input.UserInputState == Enum.UserInputState.End then
					Dragging = false
				end
			end)
		end
	end)
	--
	Topbar.InputChanged:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseMovement or Input.UserInputType == Enum.UserInputType.Touch then
			DragInput = Input
		end
	end)
	--
	game:GetService('UserInputService').InputChanged:Connect(function(Input)
		if Input == DragInput and Dragging then
			Update(Input)
		end
	end)
end

function MakeBorder(Object, Radius, Thickness)
	local Corner = Instance.new('UICorner', Object)
	Corner.Name = 'Corner'
	Corner.CornerRadius = Radius
	local Stroke = Instance.new('UIStroke', Object)
	Stroke.Name = 'Stroke'
	Stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
	Stroke.Color = Color3.fromRGB(100, 100, 100)
	Stroke.LineJoinMode = Enum.LineJoinMode.Round
	Stroke.Thickness = Thickness
end

-- Important
function Sosejianaki:NewWindow(Parameters)
	-- Installations
	local Important = Instance.new('Frame', Library)
	Important.Name = 'Frame'
	Important.AnchorPoint = Vector2.new(.5, .5)
	Important.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
	Important.BackgroundTransparency = 0
	Important.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Important.BorderSizePixel = 0
	Important.Position = UDim2.new(.5, 0, .5, 0)
	Important.Size = UDim2.new(0, 600, 0, 350)
	Important.ZIndex = 5
	MakeBorder(Important, UDim.new(0, 5), .75)
	--
	local Topbar = Instance.new('Frame', Important)
	Topbar.Name = 'Frame'
	Topbar.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
	Topbar.BackgroundTransparency = 0
	Topbar.BorderColor3 = Color3.fromRGB(0, 0, 0)
