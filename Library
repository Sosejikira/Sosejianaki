			ContentImage.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ContentImage.BorderSizePixel = 0
			ContentImage.Position = UDim2.new(0, 405, .5, 0)
			ContentImage.Size = UDim2.new(0, 20, 0, 20)
			ContentImage.ZIndex = 5
			ContentImage.Image = 'rbxassetid://7072707153' --:Disabled()
			ContentImage.ImageColor3 = Color3.fromRGB(250, 250, 250)

			-- Connections
			ContentButton.MouseButton1Click:Connect(function()
				if Toggled == false then
					ContentImage.Image = 'rbxassetid://7072706536' --:Enabled()
				else
					ContentImage.Image = 'rbxassetid://7072707153' --:Disabled()
				end
				Toggled = not Toggled
				pcall(Parameters['Callback'], Toggled)
			end)

			-- Default
			if Parameters['Default'] == true then
				ContentImage.Image = 'rbxassetid://7072706536' --:Enabled()
				Toggled = not Toggled
				pcall(Parameters['Callback'], Toggled)
			end

			-- Canvas
			local NewCanvasSize = UDim2.new(0, 0, 0, ContainerLayout.AbsoluteContentSize.Y)
			game:GetService('TweenService'):Create(NewContainer, TweenInfo.new(.25), {CanvasSize = NewCanvasSize}):Play()
		end
		function Contents:AddSlider(Parameters)
			local Slider = {}
			-- Functions
			local Dragging = false

			-- Installations
			local ContentFrame = Instance.new('Frame', NewContainer)
			ContentFrame.Name = 'Frame'
			ContentFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			ContentFrame.BackgroundTransparency = 1
			ContentFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ContentFrame.BorderSizePixel = 0
			ContentFrame.Size = UDim2.new(0, 450, 0, 60)
			ContentFrame.ZIndex = 5
