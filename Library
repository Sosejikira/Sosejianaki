	MakeBorder(Topbar, UDim.new(0, 5), .75)
	--
	local Frame = Instance.new('TextLabel', Topbar)
	Frame.Name = 'Frame'
	Frame.BackgroundColor3 = Color3.new(0, 0, 0)
	Frame.BackgroundTransparency = 1
	Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Frame.BorderSizePixel = 0
	Frame.Position = UDim2.new(0, 5, 0, 0)
	Frame.Size = UDim2.new(0, 250, 0, 25)
	Frame.ZIndex = 5
	Frame.Font = Enum.Font.Gotham
	Frame.Text = Parameters['Name']
	Frame.TextColor3 = Color3.fromRGB(250, 250, 250)
	Frame.TextSize = 15
	Frame.TextXAlignment = Enum.TextXAlignment.Left
	Frame.TextYAlignment = Enum.TextYAlignment.Center
	--
	local Image = Instance.new('ImageButton', Topbar)
	Image.Name = 'Destroy'
