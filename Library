		function Contents:AddBind(Parameters)
			-- Functions
			local Keybind = Parameters['Preset'].Name
			
			-- Installations
			local ContentFrame = Instance.new('Frame', NewContainer)
			ContentFrame.Name = 'Frame'
			ContentFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			ContentFrame.BackgroundTransparency = 1
			ContentFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ContentFrame.BorderSizePixel = 0
			ContentFrame.Size = UDim2.new(0, 450, 0, 50)
			ContentFrame.ZIndex = 5
			--
			local ContentButton = Instance.new('TextButton', ContentFrame)
			ContentButton.Name = 'Button'
			ContentButton.AnchorPoint = Vector2.new(.5, .5)
			ContentButton.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
			ContentButton.BackgroundTransparency = 0
			ContentButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ContentButton.BorderSizePixel = 0
			ContentButton.Position = UDim2.new(.5, 0, .5, 0)
			ContentButton.Size = UDim2.new(0, 440, 0, 40)
			ContentButton.ZIndex = 5
			ContentButton.Text = ''
			ContentButton.Font = Enum.Font.Gotham
			ContentButton.TextColor3 = Color3.fromRGB(0, 0, 0)
			ContentButton.TextSize = 15
			MakeBorder(ContentButton, UDim.new(0, 10), .75)
			--
			local ContentLabel = Instance.new('TextLabel', ContentButton)
			ContentLabel.Name = 'Frame'
			ContentLabel.AnchorPoint = Vector2.new(.5, .5)
			ContentLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			ContentLabel.BackgroundTransparency = 1
			ContentLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ContentLabel.BorderSizePixel = 0
			ContentLabel.Position = UDim2.new(.50, 0, .50, 0)
			ContentLabel.Size = UDim2.new(0, 420, 0, 40)
			ContentLabel.ZIndex = 5
			ContentLabel.Font = Enum.Font.Gotham
			ContentLabel.Text = Parameters['Name']
			ContentLabel.TextColor3 = Color3.fromRGB(250, 250, 250)
			ContentLabel.TextSize = 15
			ContentLabel.TextXAlignment = Enum.TextXAlignment.Left
			ContentLabel.TextYAlignment = Enum.TextYAlignment.Center
			--
			local ContentLabel = Instance.new('TextLabel', ContentButton)
			ContentLabel.Name = 'Frame'
			ContentLabel.AnchorPoint = Vector2.new(.5, .5)
			ContentLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			ContentLabel.BackgroundTransparency = 1
			ContentLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ContentLabel.BorderSizePixel = 0
			ContentLabel.Position = UDim2.new(.50, 0, .50, 0)
			ContentLabel.Size = UDim2.new(0, 420, 0, 40)
			ContentLabel.ZIndex = 5
			ContentLabel.Font = Enum.Font.Gotham
			ContentLabel.Text = Keybind
			ContentLabel.TextColor3 = Color3.fromRGB(250, 250, 250)
			ContentLabel.TextSize = 15
			ContentLabel.TextXAlignment = Enum.TextXAlignment.Right
			ContentLabel.TextYAlignment = Enum.TextYAlignment.Center
			
			-- Connections
			ContentButton.MouseButton1Click:Connect(function()
				ContentLabel.Text = '...'
				local InputBegan = game:GetService("UserInputService").InputBegan:Wait()
				if InputBegan.KeyCode.Name ~= "Unknown" then
					ContentLabel.Text = InputBegan.KeyCode.Name
					Keybind = InputBegan.KeyCode.Name
				end
			end)
			
			game:GetService("UserInputService").InputBegan:Connect(function(Input, Typing)
				if Typing then return end
				if Input.KeyCode.Name == Keybind then
					pcall(Parameters['Callback'])
				end
			end)
			
			-- Canvas
			local NewCanvasSize = UDim2.new(0, 0, 0, ContainerLayout.AbsoluteContentSize.Y)
			game:GetService('TweenService'):Create(NewContainer, TweenInfo.new(.25), {CanvasSize = NewCanvasSize}):Play()
		end
