			ContentValue.Name = 'Frame'
			ContentValue.AnchorPoint = Vector2.new(.5, .15)
			ContentValue.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			ContentValue.BackgroundTransparency = 1
			ContentValue.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ContentValue.BorderSizePixel = 0
			ContentValue.Position = UDim2.new(.5, 0, .15, 0)
			ContentValue.Size = UDim2.new(0, 420, 0, 25)
			ContentValue.ZIndex = 5
			ContentValue.Font = Enum.Font.Gotham
			ContentValue.Text = tostring(Parameters['Default'] and math.floor((Parameters['Default'] / Parameters['Maximum']) * (Parameters['Maximum'] - Parameters['Minimum']) + Parameters['Minimum']) or 0)
			ContentValue.TextColor3 = Color3.fromRGB(250, 250, 250)
			ContentValue.TextSize = 15
			ContentValue.TextXAlignment = Enum.TextXAlignment.Right
			ContentValue.TextYAlignment = Enum.TextYAlignment.Center
			--
			local ContentLabel = Instance.new('TextLabel', ContentLabel)
			ContentLabel.Name = 'Frame'
			ContentLabel.AnchorPoint = Vector2.new(.5, .15)
			ContentLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			ContentLabel.BackgroundTransparency = 1
			ContentLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ContentLabel.BorderSizePixel = 0
			ContentLabel.Position = UDim2.new(.5, 0, .15, 0)
			ContentLabel.Size = UDim2.new(0, 420, 0, 25)
			ContentLabel.ZIndex = 5
			ContentLabel.Font = Enum.Font.Gotham
			ContentLabel.Text = Parameters['Name']
			ContentLabel.TextColor3 = Color3.fromRGB(250, 250, 250)
			ContentLabel.TextSize = 15
			ContentLabel.TextXAlignment = Enum.TextXAlignment.Left
			ContentLabel.TextYAlignment = Enum.TextYAlignment.Center
			--
			local ContentFrame = Instance.new('Frame', ContentFrame)
			ContentFrame.Name = 'Frame'
			ContentFrame.AnchorPoint = Vector2.new(.5, .75)
			ContentFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
			ContentFrame.BackgroundTransparency = 0
			ContentFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ContentFrame.BorderSizePixel = 0
			ContentFrame.Position = UDim2.new(.5, 0, .75, 0)
			ContentFrame.Size = UDim2.new(0, 410, 0, 5)
			ContentFrame.ZIndex = 5
			local Corner = Instance.new('UICorner', ContentFrame)
			Corner.Name = 'Corner'
			Corner.CornerRadius = UDim.new(10, 0)
			--
			local ContentSlider = Instance.new('Frame', ContentFrame)
			ContentSlider.Name = 'Frame'
			ContentSlider.BackgroundColor3 = Color3.fromRGB(95, 95, 95)
			ContentSlider.BackgroundTransparency = 0
			ContentSlider.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ContentSlider.BorderSizePixel = 0
			ContentSlider.Size = UDim2.new(0, 0, 0, 5)
			ContentSlider.ZIndex = 5
			local Corner = Instance.new('UICorner', ContentSlider)
			Corner.Name = 'Corner'
			Corner.CornerRadius = UDim.new(10, 0)
			--
			local ContentSphere = Instance.new('Frame', ContentFrame)
			ContentSphere.Name = 'Frame'
			ContentSphere.AnchorPoint = Vector2.new(0, .5)
			ContentSphere.BackgroundColor3 = Color3.fromRGB(95, 95, 95)
			ContentSphere.BackgroundTransparency = 0
			ContentSphere.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ContentSphere.BorderSizePixel = 0
			ContentSphere.Position = UDim2.new(0, 0, .5, 0)
			ContentSphere.Size = UDim2.new(0, 10, 0, 10)
			ContentSphere.ZIndex = 5
			local Corner = Instance.new('UICorner', ContentSphere)
			Corner.Name = 'Corner'
			Corner.CornerRadius = UDim.new(10, 0)

			-- Functions
			local function Move(Input)
				local Position = UDim2.new(math.clamp((Input.Position.X - ContentFrame.AbsolutePosition.X)/ContentFrame.AbsoluteSize.X, 0, 1), -6, .50, 0)
				ContentSlider:TweenSize(UDim2.new(math.clamp((Input.Position.X - ContentFrame.AbsolutePosition.X)/ContentFrame.AbsoluteSize.X, 0, 1), 0, 0, 5), "Out", "Sine", .25, true)
				ContentSphere:TweenPosition(Position, "Out", "Sine", .25, true)
				local Current = math.floor(((Position.X.Scale * Parameters['Maximum']) / Parameters['Maximum']) * (Parameters['Maximum'] - Parameters['Minimum']) + Parameters['Minimum'])
				ContentValue.Text = tostring(Current)
				pcall(Parameters['Callback'], Current)
			end

			-- Connections
			ContentSphere.InputBegan:Connect(function(Input)
				if Input.UserInputType == Enum.UserInputType.MouseButton1 then
					Dragging = true
				end
			end)
			ContentSphere.InputEnded:Connect(function(Input)
				if Input.UserInputType == Enum.UserInputType.MouseButton1 then
					Dragging = false
				end
			end)
			game:GetService("UserInputService").InputChanged:Connect(function(Input)
				if Dragging and Input.UserInputType == Enum.UserInputType.MouseMovement then
					Move(Input)
				end
			end)

			-- Canvas
			local NewCanvasSize = UDim2.new(0, 0, 0, ContainerLayout.AbsoluteContentSize.Y)
			game:GetService('TweenService'):Create(NewContainer, TweenInfo.new(.25), {CanvasSize = NewCanvasSize}):Play()
			return Slider
		end
		function Contents:AddDropdown(Parameters)
			-- Functions
			local Dropdown = {}
			local Selection = '...'
			local Toggled = false
			local DropdownSize = 50
			local Objects = 0

			-- Installations
			local ContentFrame = Instance.new('Frame', NewContainer)
			ContentFrame.Name = 'Frame'
			ContentFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			ContentFrame.BackgroundTransparency = 1
			ContentFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ContentFrame.BorderSizePixel = 0
			ContentFrame.Size = UDim2.new(0, 450, 0, 50)
			ContentFrame.ZIndex = 5
			--
			local BorderContentFrame = Instance.new('Frame', ContentFrame)
			BorderContentFrame.Name = 'Frame'
			BorderContentFrame.AnchorPoint = Vector2.new(.5, .5)
