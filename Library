			ContentLabel.TextYAlignment = Enum.TextYAlignment.Center
			local Corner = Instance.new('UICorner', ContentLabel)
			Corner.Name = 'Corner'
			Corner.CornerRadius = UDim.new(0, 10)
			--
			local ContentImage = Instance.new('ImageButton', ContentLabel)
			ContentImage.Name = 'Image'
			ContentImage.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			ContentImage.BackgroundTransparency = 1
			ContentImage.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ContentImage.BorderSizePixel = 0
			ContentImage.Position = UDim2.new(0, 405, 0, 10)
			ContentImage.Size = UDim2.new(0, 20, 0, 20)
			ContentImage.ZIndex = 5
			ContentImage.Image = 'rbxassetid://7072981875'
			ContentImage.ImageColor3 = Color3.fromRGB(250, 250, 250)
			--
			local ContentObject = Instance.new('TextLabel', ContentLabel)
			ContentObject.Name = 'Frame'
			ContentObject.AnchorPoint = Vector2.new(.75, 0)
			ContentObject.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			ContentObject.BackgroundTransparency = 1
			ContentObject.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ContentObject.BorderSizePixel = 0
			ContentObject.Position = UDim2.new(.75, 0, 0, 5)
			ContentObject.Size = UDim2.new(0, 275, 0, 30)
			ContentObject.ZIndex = 5
			ContentObject.Text = Selection
			ContentObject.Font = Enum.Font.Gotham
			ContentObject.TextColor3 = Color3.fromRGB(250, 250, 250)
			ContentObject.TextSize = 15
			ContentObject.TextXAlignment = Enum.TextXAlignment.Right
			ContentObject.TextYAlignment = Enum.TextYAlignment.Center
			--
			local ContentLabel = Instance.new('TextLabel', ContentLabel)
			ContentLabel.Name = 'Frame'
			ContentLabel.AnchorPoint = Vector2.new(.5, 0)
			ContentLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			ContentLabel.BackgroundTransparency = 1
			ContentLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ContentLabel.BorderSizePixel = 0
			ContentLabel.Position = UDim2.new(.5, 0, 0, 5)
			ContentLabel.Size = UDim2.new(0, 420, 0, 30)
			ContentLabel.ZIndex = 5
			ContentLabel.Text = Parameters['Name']
			ContentLabel.Font = Enum.Font.Gotham
			ContentLabel.TextColor3 = Color3.fromRGB(250, 250, 250)
			ContentLabel.TextSize = 15
			ContentLabel.TextXAlignment = Enum.TextXAlignment.Left
			ContentLabel.TextYAlignment = Enum.TextYAlignment.Center

			-- Connections
			ContentImage.MouseButton1Click:Connect(function()
				if Toggled == false then
					game:GetService('TweenService'):Create(ContentImage, TweenInfo.new(.25), {Rotation = 180}):Play()
					game:GetService('TweenService'):Create(ContentFrame, TweenInfo.new(.25), {Size = UDim2.new(0, 450, 0, DropdownSize)}):Play()
					game:GetService('TweenService'):Create(BorderContentFrame, TweenInfo.new(.25), {Size = UDim2.new(0, 440, 0, DropdownSize - 10)}):Play()
					game:GetService('TweenService'):Create(ContentScrolling, TweenInfo.new(.25), {Size = UDim2.new(0, 440, 0, DropdownSize - 10)}):Play()
				else
					game:GetService('TweenService'):Create(ContentImage, TweenInfo.new(.25), {Rotation = 0}):Play()
					game:GetService('TweenService'):Create(ContentFrame, TweenInfo.new(.25), {Size = UDim2.new(0, 450, 0, 50)}):Play()
					game:GetService('TweenService'):Create(BorderContentFrame, TweenInfo.new(.25), {Size = UDim2.new(0, 440, 0, 40)}):Play()
					game:GetService('TweenService'):Create(ContentScrolling, TweenInfo.new(.25), {Size = UDim2.new(0, 440, 0, 40)}):Play()
				end
				wait(.5)
				local NewCanvasSize = UDim2.new(0, 0, 0, ContainerLayout.AbsoluteContentSize.Y)
				game:GetService('TweenService'):Create(NewContainer, TweenInfo.new(.25), {CanvasSize = NewCanvasSize}):Play()
				Toggled = not Toggled
			end)

			-- Canvas
			local NewCanvasSize = UDim2.new(0, 0, 0, ContainerLayout.AbsoluteContentSize.Y)
			game:GetService('TweenService'):Create(NewContainer, TweenInfo.new(.25), {CanvasSize = NewCanvasSize}):Play()
			for Key, Children in Parameters['Options'] do
				Objects = Objects + 1
				if Objects == 1 then
					DropdownSize = 75
				elseif Objects == 2 then
					DropdownSize = 100
				elseif Objects == 3 then
					DropdownSize = 125
				elseif Objects >= 4 then
					DropdownSize = 150
				end

				-- Functions				
				local Object = Instance.new('TextButton', ContentDropdown)
				Object.Name = 'Object'
				Object.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				Object.BackgroundTransparency = 1
				Object.BorderColor3 = Color3.fromRGB(0, 0, 0)
				Object.BorderSizePixel = 0
				Object.Size = UDim2.new(0, 425, 0, 25)
				Object.ZIndex = 5
				Object.Font = Enum.Font.Gotham
				Object.Text = Children
				Object.TextColor3 = Color3.fromRGB(250, 250, 250)
				Object.TextSize = 15
				Object.TextXAlignment = Enum.TextXAlignment.Left
				Object.TextYAlignment = Enum.TextYAlignment.Center
				local Corner = Instance.new("UICorner")
				Corner.CornerRadius = UDim.new(0, 10)
				Corner.Name = "Corner"
				Corner.Parent = Object
				local NewCanvasSize = UDim2.new(0, 0, 0, DropdownLayout.AbsoluteContentSize.Y)
				game:GetService('TweenService'):Create(ContentDropdown, TweenInfo.new(.25), {CanvasSize = NewCanvasSize}):Play()

				Object.MouseButton1Click:Connect(function()
					Toggled = not Toggled
					pcall(Parameters['Callback'], Children)
					game:GetService('TweenService'):Create(ContentImage, TweenInfo.new(.25), {Rotation = 0}):Play()
					game:GetService('TweenService'):Create(ContentFrame, TweenInfo.new(.25), {Size = UDim2.new(0, 450, 0, 50)}):Play()
					game:GetService('TweenService'):Create(BorderContentFrame, TweenInfo.new(.25), {Size = UDim2.new(0, 440, 0, 40)}):Play()
					game:GetService('TweenService'):Create(ContentScrolling, TweenInfo.new(.25), {Size = UDim2.new(0, 440, 0, 40)}):Play()
					wait(.5)
					local NewCanvasSize = UDim2.new(0, 0, 0, ContainerLayout.AbsoluteContentSize.Y)
					game:GetService('TweenService'):Create(NewContainer, TweenInfo.new(.25), {CanvasSize = NewCanvasSize}):Play()
					ContentObject.Text = Children
					Selection = Children
				end)
			end
			function Dropdown:Add(Name)
				Objects = Objects + 1
				if Objects == 1 then
					DropdownSize = 75
				elseif Objects == 2 then
					DropdownSize = 100
				elseif Objects == 3 then
					DropdownSize = 125
				elseif Objects >= 4 then
					DropdownSize = 150
				end

				local Object = Instance.new('TextButton', ContentDropdown)
				Object.Name = 'Object'
				Object.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				Object.BackgroundTransparency = 1
				Object.BorderColor3 = Color3.fromRGB(0, 0, 0)
				Object.BorderSizePixel = 0
				Object.Size = UDim2.new(0, 425, 0, 25)
				Object.ZIndex = 5
				Object.Font = Enum.Font.Gotham
				Object.Text = Name
				Object.TextColor3 = Color3.fromRGB(250, 250, 250)
				Object.TextSize = 15
				Object.TextXAlignment = Enum.TextXAlignment.Left
				Object.TextYAlignment = Enum.TextYAlignment.Center
				local Corner = Instance.new("UICorner")
				Corner.CornerRadius = UDim.new(0, 10)
				Corner.Name = "Corner"
				Corner.Parent = Object
				local NewCanvasSize = UDim2.new(0, 0, 0, DropdownLayout.AbsoluteContentSize.Y)
				game:GetService('TweenService'):Create(ContentDropdown, TweenInfo.new(.25), {CanvasSize = NewCanvasSize}):Play()

				Object.MouseButton1Click:Connect(function()
					Toggled = not Toggled
					pcall(Parameters['Callback'], Name)
					ContentObject.Text = Name
					Selection = Name
					--
					game:GetService('TweenService'):Create(ContentImage, TweenInfo.new(.25), {Rotation = 0}):Play()
					game:GetService('TweenService'):Create(ContentFrame, TweenInfo.new(.25), {Size = UDim2.new(0, 450, 0, 50)}):Play()
					game:GetService('TweenService'):Create(BorderContentFrame, TweenInfo.new(.25), {Size = UDim2.new(0, 440, 0, 40)}):Play()
					game:GetService('TweenService'):Create(ContentScrolling, TweenInfo.new(.25), {Size = UDim2.new(0, 440, 0, 40)}):Play()
					wait(.5)
					local NewCanvasSize = UDim2.new(0, 0, 0, ContainerLayout.AbsoluteContentSize.Y)
					game:GetService('TweenService'):Create(NewContainer, TweenInfo.new(.25), {CanvasSize = NewCanvasSize}):Play()
				end)
				if Toggled == true then
					ContentObject.Text = Selection
					game:GetService('TweenService'):Create(ContentImage, TweenInfo.new(.25), {Rotation = 180}):Play()
					game:GetService('TweenService'):Create(ContentFrame, TweenInfo.new(.25), {Size = UDim2.new(0, 450, 0, DropdownSize)}):Play()
					game:GetService('TweenService'):Create(BorderContentFrame, TweenInfo.new(.25), {Size = UDim2.new(0, 440, 0, DropdownSize - 10)}):Play()
					game:GetService('TweenService'):Create(ContentScrolling, TweenInfo.new(.25), {Size = UDim2.new(0, 440, 0, DropdownSize - 10)}):Play()
					wait(.5)
					local NewCanvasSize = UDim2.new(0, 0, 0, ContainerLayout.AbsoluteContentSize.Y)
					game:GetService('TweenService'):Create(NewContainer, TweenInfo.new(.25), {CanvasSize = NewCanvasSize}):Play()
				end
			end
			function Dropdown:Clear()
				Objects = 0
				for Key, Children in next, ContentDropdown:GetChildren() do
					if Children.Name == "Object" then
						Children:Destroy()
					end
				end
				DropdownSize = 50
				Selection = '...'
				ContentObject.Text = Selection
				if Toggled == true then
					Toggled = not Toggled
					game:GetService('TweenService'):Create(ContentImage, TweenInfo.new(.25), {Rotation = 0}):Play()
					game:GetService('TweenService'):Create(ContentFrame, TweenInfo.new(.25), {Size = UDim2.new(0, 450, 0, 50)}):Play()
					game:GetService('TweenService'):Create(BorderContentFrame, TweenInfo.new(.25), {Size = UDim2.new(0, 440, 0, 40)}):Play()
					game:GetService('TweenService'):Create(ContentScrolling, TweenInfo.new(.25), {Size = UDim2.new(0, 440, 0, 40)}):Play()
					wait(.5)
					local NewCanvasSize = UDim2.new(0, 0, 0, ContainerLayout.AbsoluteContentSize.Y)
					game:GetService('TweenService'):Create(NewContainer, TweenInfo.new(.25), {CanvasSize = NewCanvasSize}):Play()
				end
				local NewCanvasSize = UDim2.new(0, 0, 0, DropdownLayout.AbsoluteContentSize.Y)
				game:GetService('TweenService'):Create(ContentDropdown, TweenInfo.new(.25), {CanvasSize = NewCanvasSize}):Play()
			end
			return Dropdown
		end
